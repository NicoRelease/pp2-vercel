{
  "version": 2,
  "builds": [
    { "src": "Backend/api/index.js", "use": "@vercel/node" },
    { "src": "FrontEnd/package.json", "use": "@vercel/static-build" }
  ],
  "rewrites": [
    { 
      "source": "/backend/:path*", 
      "destination": "/Backend/api/index.js" 
    },
    { 
      "source": "/assets/:path*", 
      "destination": "/FrontEnd/dist/assets/:path*" 
    },
    { 
      "source": "/(.*)", 
      "destination": "/FrontEnd/dist/index.html" 
    }
  ]
}