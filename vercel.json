{
  "version": 2,
  "builds": [
    {
      "src": "Backend/api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "FrontEnd/package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/backend/(.*)",
      "dest": "Backend/api/index.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/FrontEnd/dist/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/FrontEnd/dist/index.html"
    }
  ]
}